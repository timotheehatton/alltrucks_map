{
  "expo": {
    "name": "Alltrucks",
    "slug": "alltrucks-workshop",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.9.8",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#051A3D"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alltrucks.workshopapp",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Alltrucks would like to use your current location to display it on the app and find the closest workshop",
        "NSLocationAlwaysUsageDescription": "Alltrucks would like to use your current location to display it on the app and find the closest workshop"
      }
    },
    "android": {
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE"
      ],
      "versionCode": 30,
      "splash": {
        "backgroundColor": "#051A3D",
        "mdpi": "./assets/images/splash_mdpi.png",
        "hdpi": "./assets/images/splash_ldpi.png",
        "ldpi": "./assets/images/splash_hdpi.png",
        "xhdpi": "./assets/images/splash_xhdpi.png",
        "xxhdpi": "./assets/images/splash_xxhdpi.png",
        "xxxhdpi": "./assets/images/splash_xxxhdpi.png"
      },
      "package": "com.alltrucks.workshopapp",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDm0zmmHdAG4O9z9BDSV0TeNJxof1x86lw"
        }
      }
    },
    "plugins": [
      "expo-font",
      "expo-asset"
    ]
  }
}
